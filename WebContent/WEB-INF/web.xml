<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" version="3.0">
  <servlet>
    <servlet-name>ClientServlet</servlet-name>
    <servlet-class>com.campusnumerique.vehiclerental.servlet.client.ClientServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ClientServlet</servlet-name>
    <url-pattern>/clients</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>CarServlet</servlet-name>
    <servlet-class>com.campusnumerique.vehiclerental.servlet.car.CarServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>CarServlet</servlet-name>
    <url-pattern>/cars</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>ReservationServlet</servlet-name>
    <servlet-class>com.campusnumerique.vehiclerental.servlet.reservation.ReservationServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ReservationServlet</servlet-name>
    <url-pattern>/reservation</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>SelectVehicleServlet</servlet-name>
    <servlet-class>com.campusnumerique.vehiclerental.servlet.selectvehicle.SelectVehicleServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>SelectVehicleServlet</servlet-name>
    <url-pattern>/selection</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>clients</servlet-name>
    <jsp-file>/WEB-INF/pages/clients.jsp</jsp-file>
  </servlet>
  <servlet>
    <servlet-name>cars</servlet-name>
    <jsp-file>/WEB-INF/pages/cars.jsp</jsp-file>
  </servlet>
  <servlet>
    <servlet-name>reservation</servlet-name>
    <jsp-file>/WEB-INF/pages/reservation.jsp</jsp-file>
  </servlet>
  <servlet>
    <servlet-name>selectVehicle</servlet-name>
    <jsp-file>/WEB-INF/pages/selectVehicle.jsp</jsp-file>
  </servlet>
</web-app>